<h1>Sign up!</h1> 

<%= render partial: 'layouts/errors', locals: {object: @user}   %>

<%= form_for(@user) do |f| %>
    <%= f.label :username %><br>
    <%= f.text_field :username %><br>

    <%= f.label :password %><br>
    <%= f.password_field :password %><br>

    <%= f.submit "Sign Up" %>
    <% end %>